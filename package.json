{
  "name": "bubblewap",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo app/boot.js -t babelify -d --serve bundle.js --live",
    "build": "browserify app/boot.js -t babelify -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/irenerobb72/bubblewap"
  },
  "author": "irenerobb72",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/irenerobb72/bubblewap/issues"
  },
  "homepage": "https://github.com/irenerobb72/bubblewap",
  "dependencies": {
    "jquery": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  },
  "jspm": {
    "dependencies": {
      "jsx": "npm:jspm-loader-jsx@^0.0.7",
      "react": "npm:react@^0.14.7",
      "react-dom": "npm:react-dom@^0.14.7"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
